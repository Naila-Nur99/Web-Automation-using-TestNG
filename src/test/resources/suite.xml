<?xml version="1.0" encoding="UTF-8" ?>
<suite name="FinancialWebTestSuite">

    <test name="User Get Congratulations Email after Registration">
        <classes>
            <class name="testrunner.CongratulationsEmailTestRunner"></class>
        </classes>
    </test>

    <test name="Attampts to Reset Password">
        <classes>
            <class name="testrunner.ResetPasswrdTestRunner"></class>
        </classes>
    </test>

    <test name="User Login with New Password">
        <classes>
            <class name="testrunner.UserLoginNewPassTestRunner"></class>
        </classes>
    </test>


    <test name="Add 2 items:Item-1 with all fields,Item-2 with only mandatory fields">
        <classes>
            <class name="testrunner.ItemCreateTestRunner"></class>
        </classes>
    </test>

    <test name="Update user Gmail with a new Gmail">
        <classes>
            <class name="testrunner.UpdateGmailTestRunner"></class>
        </classes>
    </test>


    <test name="Login with Updated Gmail is Successful and Login with Old Gmail is Failed">
        <classes>
            <class name="testrunner.LoginWithGmailTestRunner"></class>
        </classes>
    </test>


    <test name="Admin Login From Terminal and Searching the Updated User Gmail from Admin Dashboard">
        <classes>
            <class name="testrunner.AdminLoginTestRunner"></class>
        </classes>
    </test>

    <test name="Register Users getting Data from CSV file">
        <classes>
            <class name="testrunner.UserRegCsvTestRunner"></class>
        </classes>
    </test>

    <test name="Admin Get all the User from User Table">
        <classes>
            <class name="testrunner.GetAllUserTestRunner"></class>
        </classes>
    </test>



</suite>


